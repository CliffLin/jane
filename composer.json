{
    "name": "jane/jane",
    "description": "Generate a serializable / deserializable object model given a json schema",
    "license": "MIT",
    "authors": [
        {
            "name": "Joel Wurtz",
            "email": "jwurtz@jolicode.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Joli\\Jane\\": "src/",
