{
    "name": "jane/ast-generator",
    "description": "Ast generator library to convert property info object to generated code (model and normalizer)",
    "license": "MIT",
    "authors": [
        {
            "name": "Joel Wurtz",
            "email": "jwurtz@jolicode.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Joli\\Jane\\AstGenerator\\": "./"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joli\\Jane\\AstGenerator\\Tests\\": "Tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "nikic/php-parser": "^2.0",
        "doctrine/inflector": "^1.0",
        "symfony/property-info": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
